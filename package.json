{
  "name": "googlesearchbooks",
  "version": "1.0.0",
  "description": "Google book search built with react",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.22.0",
    "caniuse-lite": "^1.0.30001265",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.9.1"
  },
  "devDependencies": {},
  "scripts": {
    "start": "if-env NODE_ENV=production && npm start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "npm run client",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pedrazajulian/GoogleSearchBooks.git"
  },
  "author": "Julian Pedraza",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pedrazajulian/GoogleSearchBooks/issues"
  },
  "homepage": "https://github.com/pedrazajulian/GoogleSearchBooks#readme"
}

